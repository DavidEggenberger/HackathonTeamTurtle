<div class="card">
    <div class="card-body">
        <h4 style="margin-bottom: 5px; margin-top: 5px;">Add a link to an external video</h4>
        <label>URI:</label>
        <input @bind="Node.LearningRessourceDTO.LinkURI"/>
    </div>
    @foreach (var port in Node.Ports)
    {
        <PortRenderer Port="port" />
    }
</div>

@code{
    [Parameter]
    public VideoExternNode Node { get; set; }
}
